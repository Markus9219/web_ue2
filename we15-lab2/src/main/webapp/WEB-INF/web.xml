<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
	
    <welcome-file-list>
        <welcome-file>login.xhtml</welcome-file>
    </welcome-file-list>
    
    <servlet>   
        <display-name>BigJeopardyServlet</display-name>
        <servlet-name>BigJeopardyServlet</servlet-name>
        <servlet-class>at.ac.tuwien.big.we15.lab2.servlet.BigJeopardyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BigJeopardyServlet</servlet-name>
        <url-pattern>/BigJeopardyServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>   
        <display-name>HelloWorld</display-name>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>at.ac.tuwien.big.we15.lab2.servlet.HelloWorld</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
</web-app>